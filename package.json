{
  "name": "baby-book",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run process-books && tsc && vite build",
    "build:fast": "tsc && vite build",
    "process-books": "node scripts/book-scanner.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 5",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "npm run build && npm run deploy:gh-pages",
    "deploy:gh-pages": "npx gh-pages -d dist"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.35.3",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.288.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.16.0",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.1.1",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
